 Cell[BoxData[
  ButtonBox["HCSupercellModelGraph",
   BaseStyle->"Link",
   ButtonData->
    "paclet:PatrickMLenggenhager/HyperBloch/ref/HCSupercellModelGraph"]], 
  "InlineFormula",ExpressionUUID->"245723ba-24ad-451d-ac33-4fc20155dc3a"],
 " ",
 Cell[BoxData[
  StyleBox["mgraph", "TI"]], "InlineFormula",ExpressionUUID->
  "5b3907b5-7d31-4ef5-80de-5e89cf64802f"],
 " with the number of orbitals at each site specified by ",
 Cell[BoxData[
  StyleBox["norb", "TI"]], "InlineFormula",ExpressionUUID->
  "135a8dae-fb67-4372-b741-af2c87bc9b5a"],
 ", the onsite term by ",
 Cell[BoxData[
  StyleBox["onsite", "TI"]], "InlineFormula",ExpressionUUID->
  "c03d716f-b48d-4e22-b8a1-d6f0cf33f744"],
 ", and the hopping along an edge by ",
 Cell[BoxData[
  StyleBox["hoppings", "TI"]], "InlineFormula",ExpressionUUID->
  "a9e5825b-11c0-4701-918a-942bd7399d55"],
 " as a function ",
 StyleBox["k",
  FontSlant->"Italic"],
 " \[RuleDelayed] \[ScriptCapitalH](",
 StyleBox["k",
  FontSlant->"Italic"],
 ")"
}], "Usage",
 CellChangeTimes->{{3.9060807331780577`*^9, 3.906080823894163*^9}},
 CellID->207877341,ExpressionUUID->"5a95bc6d-6279-4563-9d22-a21d53dda161"],

