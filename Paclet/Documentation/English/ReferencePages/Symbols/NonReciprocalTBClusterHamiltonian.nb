Cell[TextData[{
 Cell["   ", "ModInfo",ExpressionUUID->"b449a0b5-3f26-461d-a709-1bebd6fab7ee"],
 Cell[BoxData[
  RowBox[{"NonReciprocalTBClusterHamiltonian", "[", 
   RowBox[{
    StyleBox["mgraph", "TI"], 
    StyleBox[",", "TI"], 
    StyleBox["norb", "TI"], ",", 
    StyleBox["onsite", "TI"], ",", 
    StyleBox["hoppingsCanonical", "TI"], ",", 
    StyleBox["hoppingsOpposite", "TI"]}], "]"}]], "InlineFormula",
  ExpressionUUID->"6c4c0ed1-1006-4499-b140-9ff19475cbdb"],
 " \[LineSeparator]returns the tight-binding Hamiltonian H of the model graph \
",
 Cell[BoxData[
  ButtonBox["HCModelGraph",
   BaseStyle->"Link",
   ButtonData->"paclet:PatrickMLenggenhager/HyperBloch/ref/HCModelGraph"]], 
  "InlineFormula",ExpressionUUID->"b93ca1e0-d007-fb4c-aa3e-92faab477aab"],
 " or ",
 Cell[BoxData[
  ButtonBox["HCSupercellModelGraph",
   BaseStyle->"Link",
   ButtonData->
    "paclet:PatrickMLenggenhager/HyperBloch/ref/HCSupercellModelGraph"]], 
  "InlineFormula",ExpressionUUID->"39453529-e3de-7f43-b2a3-a87b21467389"],
